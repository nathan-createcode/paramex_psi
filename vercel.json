{
  "buildCommand": "cd psi_paramex && npm run build",
  "outputDirectory": "psi_paramex/dist",
  "installCommand": "cd psi_paramex && npm install",
  "functions": {
    "psi_paramex/api/chat.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/psi_paramex/api/chat.py"
    }
  ]
} 